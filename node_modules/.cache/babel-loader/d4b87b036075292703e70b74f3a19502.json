  computed:{\n    sidebarItems(){\n      return [DashboardSidebarItems, ReportSidebarItems,BillsSidebarItems,SettingsSidebarItems]\n    },\n    ...mapState({\n      isdarkMode:state => state.auth.darkMode,\n\n    })\n  },\n  mounted() {\n    // this.initScrollbar()\n  }\n};\n</script>\n<style scoped>\n.header{\n  /*border: solid;*/\n}\n.sidebar-headers{\n  color: #Fefefe;\n  font-size: 14px;\n  margin: 0;\n  /*text-decoration: underline;*/\n  /*border: solid #236395 1px;*/\n  /*color: #236395;*/\n  text-align: left;\n  padding: 15px;\n  /*padding-top: 15px;*/\n  font-weight: 500;\n  letter-spacing: 1px;\n  /*background-color: rgba(35, 99, 149, 0.18);*/\n}\n\n.hide{\n  height: 0;\n  cursor: pointer;\n  transition: .5s ease;\n  opacity: 0;\n}\n.wrapper{\n  height: auto;\n  /*border: solid;*/\n  /*overflow-y: scroll;*/\n  width: 100%;\n}\n\n.nav-link-text{\n  color: white;\n  padding: 10px;\n  font-size: 14px;\n  font-weight: 600;\n  margin-left: 10px;\n}\n\n.dropdown-content{}\n\n.show{\n  height:5vh;\n  opacity: 1;\n  transition: .5s ease;\n  z-index: 1;\n  border: solid black;\n}\n\n.rotate{\n  transform: rotate(-90deg);\n  transition: .5s ease;\n  cursor: pointer;\n}\n\n.arrow{\n  font-size: 12px;\n  font-weight: 900;\n  transform: rotate(0deg);\n  color: #ffffff;\n  transition: .5s ease;\n  /*border: solid;*/\n}\n\n.content{\n  /*background-image: url(\"https://quikgem-repo.s3.amazonaws.com/15410+1.png\");*/\n  /*background-position: center;*/\n  /*background-repeat: no-repeat;*/\n  /*background-size: cover;*/\n  min-height: 100vh;\n\n}\n</style>\n"],"mappings":";;AA6CA,OAAAA,gBAAA;AACA;AACA,OAAAC,qBAAA;AACA,OAAAC,kBAAA;AACA,OAAAC,oBAAA;AACA;AACA;AACA,OAAAC,yBAAA;AACA,OAAAC,eAAA;AACA,OAAAC,eAAA;AACA,OAAAC,aAAA;AACA,SAAAC,cAAA;AACA,OAAAC,iBAAA;AACA,SAAAC,QAAA;AAEA,SAAAC,WAAAC,SAAA;EACA,OAAAC,QAAA,CAAAC,sBAAA,CAAAF,SAAA,EAAAG,MAAA;AACA;AAEA,SAAAC,eAAAJ,SAAA;EACA,IAAAD,UAAA,CAAAC,SAAA;IACA,IAAAZ,gBAAA,CAAAY,SAAA;EACA;IACAK,UAAA;MACAD,cAAA,CAAAJ,SAAA;IACA;EACA;AACA;AA