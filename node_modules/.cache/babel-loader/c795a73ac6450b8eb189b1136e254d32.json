rganisation(){\n      return StoreUtils.rootGetters(StoreUtils.getters.auth.getCurrentOrganization)\n    },\n\n    ...mapState({\n      isExpired:state => state.auth.isTimedOut,\n      addBizForm: (state) => state.auth.form,\n      isReminder:state => state.auth.reminderForm,\n      isdarkMode:state => state.auth.darkMode\n\n    })\n\n\n  },\n  mounted() {\n    setTimeout(()=>{\n      if(router.currentRoute.meta.layout !== \"auth\"){\n        const userToken = localStorage.getItem('token')\n        StoreUtils.dispatch(StoreUtils.actions.auth.revalidateUser, userToken)\n\n        this.readPayoutAccountModel.accountOrganisationId =\n            localStorage.organisationId;\n        StoreUtils.dispatch(\n            StoreUtils.actions.accountPayout.readAddedBanks,\n            this.readPayoutAccountModel\n        );\n        console.clear()\n      }else{\n        localStorage.clear()\n      }\n    },1000)\n\n      setTimeout(()=>{\n        let params = this.$route.params\n         if(params != null){\n           if(params.referralCode != null) localStorage.referralCode = params.referralCode\n          }\n      },2000)\n    // console.clear()\n  },\n\n  methods:{\n    updateAddBusiness(value) {\n      StoreUtils.commit(StoreUtils.mutations.auth.updateForm, value)\n    },\n    updateReminder(value){\n      StoreUtils.commit(StoreUtils.mutations.auth.updateReminderForm, value)\n    }\n  },\n\n}\n</script>\n\n<style>\n@import url('https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap');\n*{\n  font-family: 'Montserrat';\n  box-sizing: border-box;\n}\n\n::-webkit-scrollbar {\n  display: none;\n}\n\n::-webkit-outer-spin-button{\n  display: none;\n}\n\nbody{background-color: #FFFFFF;}\n\n.myalert{\n  background-color: #EE9BA9 !important;\n  box-shadow: 0 1px 2px 0 rgb(0 0 0 / 20%);\n  display: flex;\n  align-items: flex-end;\n  justify-content:center;\n  height: 46px;\n  width: 100%;\n  z-index: 5;\n  position: sticky;\n  top: 0;\n}\n.session{\n  background-color: #AA3349FF !important;\n  box-shadow: 0 1px 2px 0 rgb(0 0 0 / 20%);\n  height: 46px;\n  position: fixed;\n  top: 0;\n  width: 100%;\n  z-index: 5;\n  text-align: center;\n  padding: 10px;\n}\n\n.session > h3 {\n  font-size: 14px;\n  /*margin-top: 9px;*/\n}\n\n.myalert h3{\n  font-size: 13px;\n}\n\n@media (max-width: 500px) {\n  .myalert{\n    background-color: #EE9BA9 !important;\n    box-shadow: 0 1px 2px 0 rgb(0 0 0 / 20%);\n    height: 46px;\n    width: 100%;\n    z-index: 1;\n    text-align: center;\n    padding: 5px;\n  }\n\n\n\n  .myalert h3{\n    font-size: 10px;\n  }\n\n}\n</style>\n\n"],"mappings":";AAmBA,OAAAA,eAAA;AACA,OAAAC,UAAA;AACA,OAAAC,UAAA;AACA,OAAAC,oBAAA;AACA,OAAAC,MAAA;AACA,SAAAC,QAAA;AACA,OAAAC,cAAA;AACA,OAAAC,aAAA;AACA,OAAAC,aAAA;AAEA,IAAAC,cAAA;AACA;EACAC,UAAA;IAAAT,UAAA,EAAAA,UAAA;IAAAD,eAAA,EAAAA,eAAA;IAAAM,cAAA,EAAAA,cAAA;IAAAC,aAAA,EAAAA,aAAA;IAAAC,aAAA,EAAAA;EAAA;EACAG,IAAA,WAAAA,KAAA;IACA;MACAC,sBAAA,EAAAT,oBAAA,CAAAU,qBAAA;MACAC,YAAA,EAAAC,MAAA,CAAAC,KAAA,CAAAC,GAAA,CAAAH;IAEA;EACA;EACAI,QAAA,EAAAC,aAAA;IACAC,MAAA,WAAAA,OAAA;MACA,aAAAC,MAAA,CAAAC,IAAA,CAAAF,MAAA,IAAAX,cAAA;IACA;IACAc,mBAAA,WAAAA,oBAAA;MACA,OAAArB,UAAA,CAAAsB,WAAA,CAAAtB,UAAA,CAAAuB,OAAA,CAAAC,IAAA,CAAAC,sBAAA;IACA;EAAA,GAEAtB,QAAA;IACAuB,SAAA,WAAAA,UAAAC,KAAA;MAAA,OAAAA,KAAA,CAAAH,IAAA,CAAAI,UAAA;IAAA;IACAC,UAAA,WAAAA,WAAAF,KAAA;MAAA,OAAAA,KAAA,CAAAH,IAAA,CAAAM,IAAA;IAAA;IACAC,UAAA,WAAAA,WAAAJ,KAAA;MAAA,OAAAA,KAAA,CAAAH,IAAA,CAAAQ,YAAA;IAAA;IACAC,UAAA,WAAAA,WAAAN,KAAA;MAAA,OAAAA,KAAA,CAAAH,IAAA,CAAAU,QAAA;IAAA;EAEA,GAGA;EACAC,OAAA,WAAAA,QAAA;IAAA,IAAAC,KAAA;IACAC,UAAA;MACA,IAAAnC,MAAA,CAAAoC,YAAA,CAAAlB,IAAA,CAAAF,MAAA;QACA,IAAAqB,SAAA,GAAAC,YAAA,CAAAC,OAAA;QACAzC,UAAA,CAAA0C,QAAA,CAAA1C,UAAA,CAAA2C,OAAA,CAAAnB,IAAA,CAAAoB,cAAA,EAAAL,SAAA;QAEAH,KAAA,CAAA1B,sBAAA,CAAAmC,qBAAA,GACAL,YAAA,CAAAM,cAAA;QACA9C,UAAA,CAAA0C,QAAA,CACA1C,UAAA,CAAA2C,OAAA,CAAAI,aAAA,